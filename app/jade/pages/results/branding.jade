block var
	- var title = 'Результаты 1-го этапа секции «Брендинг»'
extends ../../layout/default.jade
block header
	include ../../partials/navbar.jade
mixin team(winner, team, scores)
	if winner
		tr
			td.results__winner
			td.results__team= team
			td
			td
			td
			td
			td
			td= scores
	else
		tr.results__loser
			td
			td.results__team= team
			td
			td
			td
			td
			td
			td= scores
